<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="InputParam" table="input_param">
    <indexes>
      <index name="input_param_params_set_id_foreign" columns="params_set_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="number" type="integer" column="number" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value" type="string" column="value" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="paramsSet" target-entity="ParamsSet" fetch="LAZY">
      <join-columns>
        <join-column name="params_set_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
