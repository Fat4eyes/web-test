<?php



/**
 * RoleUser
 */
class RoleUser
{
/**
 * @var integer
 */
private $id;

/**
 * @var \Role
 */
private $role;

/**
 * @var \User
 */
private $user;


/**
 * Get id
 *
 * @return integer
 */
public function getId()
{
return $this->id;
}

/**
 * Set role
 *
 * @param \Role $role
 *
 * @return RoleUser
 */
public function setRole(\Role $role = null)
{
$this->role = $role;

return $this;
}

/**
 * Get role
 *
 * @return \Role
 */
public function getRole()
{
return $this->role;
}

/**
 * Set user
 *
 * @param \User $user
 *
 * @return RoleUser
 */
public function setUser(\User $user = null)
{
$this->user = $user;

return $this;
}

/**
 * Get user
 *
 * @return \User
 */
public function getUser()
{
return $this->user;
}
}
